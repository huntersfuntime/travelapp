{
  "name": "travelapp",
  "version": "1.0.0",
  "description": "Landing page for Natours",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "compile:sass": "node-sass sass/main.scss css/style..comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' style.concat.css -o css/style/prefix.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huntersfuntime/travelapp.git"
  },
  "author": "\"Hunter\"",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/huntersfuntime/travelapp/issues"
  },
  "homepage": "https://github.com/huntersfuntime/travelapp#readme",
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "node-sass": "^4.7.2",
    "postcss-cli": "^4.1.1"
  }
}
